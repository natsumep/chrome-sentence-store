  "version": "1.0",
  "manifest_version": 2,
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "author":"pp",
  "background": {
    "scripts": ["index.js"],
    "persistent": false
